---
 3rdparty/qlogind/src/CMakeLists.txt | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/3rdparty/qlogind/src/CMakeLists.txt b/3rdparty/qlogind/src/CMakeLists.txt
index 7643e43..b6a326b 100644
--- a/3rdparty/qlogind/src/CMakeLists.txt
+++ b/3rdparty/qlogind/src/CMakeLists.txt
@@ -4,7 +4,7 @@ macro(generateInterface interfaceName)
     string(REPLACE "." "" fileName ${interfaceName})
 
     add_custom_command(OUTPUT ${fileName}.cpp OUTPUT ${fileName}.h
-        COMMAND ../tools/enhancedqdbusxml2cpp -p ${fileName} ${CMAKE_SOURCE_DIR}/3rdparty/qlogind/spec/${interfaceName}.xml -i types.h
+        COMMAND enhancedqdbusxml2cpp -p ${fileName} ${CMAKE_SOURCE_DIR}/3rdparty/qlogind/spec/${interfaceName}.xml -i types.h
         WORKING_DIRECTORY ${CMAKE_CURRENT_BINARY_DIR}
         COMMENT "Generating Interface for ${interfaceName}"
         SOURCES $fileName.h
