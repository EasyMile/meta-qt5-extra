Content-Type: text/plain; charset=UTF-8
Content-Transfer-Encoding: 8bit

Upstream-Status: pending

Signed-off-by: Andreas MÃ¼ller <schnitzeltony@googlemail.com>
---
 klipper/klipper.cpp | 2 ++
 klipper/main.cpp    | 3 +++
 krunner/main.cpp    | 4 ++++
 shell/main.cpp      | 4 ++++
 4 files changed, 13 insertions(+)

diff --git a/klipper/klipper.cpp b/klipper/klipper.cpp
index 79060d8..336adae 100644
--- a/klipper/klipper.cpp
+++ b/klipper/klipper.cpp
@@ -223,10 +223,12 @@ Klipper::Klipper(QObject* parent, const KSharedConfigPtr& config, KlipperMode mo
         m_popup->plugAction( m_quitAction );
     }
 
+#ifndef QT_NO_SESSIONMANAGER
     // session manager interaction
     if (m_mode == KlipperMode::Standalone) {
         connect(qApp, &QGuiApplication::commitDataRequest, this, &Klipper::saveSession);
     }
+#endif
 }
 
 Klipper::~Klipper()
diff --git a/klipper/main.cpp b/klipper/main.cpp
index 58a0778..c647d72 100644
--- a/klipper/main.cpp
+++ b/klipper/main.cpp
@@@ -27,7 +27,9 @@
 
 #include <QApplication>
 #include <QCommandLineParser>
+#ifndef QT_NO_SESSIONMANAGER
 #include <QSessionManager>
+#endif
 
 #include "tray.h"
 #include "klipper.h"
@@ -73,6 +75,7 @@ extern "C" int Q_DECL_EXPORT kdemain(int argc, char *argv[])
 
   KAboutData::setApplicationData(aboutData);
 
+#ifndef QT_NO_SESSIONMANAGER
   QGuiApplication::setFallbackSessionManagementEnabled(false);
 
   auto disableSessionManagement = [](QSessionManager &sm) {
@@ -80,6 +83,7 @@ extern "C" int Q_DECL_EXPORT kdemain(int argc, char *argv[])
   };
   QObject::connect(&app, &QGuiApplication::commitDataRequest, disableSessionManagement);
   QObject::connect(&app, &QGuiApplication::saveStateRequest, disableSessionManagement);
+#endif
   app.setQuitOnLastWindowClosed( false );
 
   QCommandLineParser parser;
diff --git a/krunner/main.cpp b/krunner/main.cpp
index b63156d..4edc1a8 100644
--- a/krunner/main.cpp
+++ b/krunner/main.cpp
@@ -24,7 +24,9 @@
 #include <QUrl>
 #include <QDebug>
 #include <QQuickWindow>
+#ifndef QT_NO_SESSIONMANAGER
 #include <QSessionManager>
+#endif
 
 #include <KAuthorized>
 #include <KAboutData>
@@ -76,6 +78,7 @@ int main(int argc, char **argv)
 
     KAboutData::setApplicationData(aboutData);
 
+#ifndef QT_NO_SESSIONMANAGER
     QGuiApplication::setFallbackSessionManagementEnabled(false);
 
     auto disableSessionManagement = [](QSessionManager &sm) {
@@ -83,6 +86,7 @@ int main(int argc, char **argv)
     };
     QObject::connect(&app, &QGuiApplication::commitDataRequest, disableSessionManagement);
     QObject::connect(&app, &QGuiApplication::saveStateRequest, disableSessionManagement);
+#endif
 
     View view;
     view.setVisible(false);
diff --git a/shell/main.cpp b/shell/main.cpp
index ec117f3..93791b2 100644
--- a/shell/main.cpp
+++ b/shell/main.cpp
@@ -21,7 +21,9 @@
 #include <QApplication>
 #include <QCommandLineParser>
 #include <QQuickWindow>
+#ifndef QT_NO_SESSIONMANAGER
 #include <QSessionManager>
+#endif
 #include <QDebug>
 #include <QMessageBox>
 
@@ -110,6 +112,7 @@ int main(int argc, char *argv[])
     cliOptions.process(app);
     aboutData.processCommandLine(&cliOptions);
 
+#ifndef QT_NO_SESSIONMANAGER
     QGuiApplication::setFallbackSessionManagementEnabled(false);
 
     auto disableSessionManagement = [](QSessionManager &sm) {
@@ -117,6 +120,7 @@ int main(int argc, char *argv[])
     };
     QObject::connect(&app, &QGuiApplication::commitDataRequest, disableSessionManagement);
     QObject::connect(&app, &QGuiApplication::saveStateRequest, disableSessionManagement);
+#endif
 
     ShellManager::s_fixedShell = cliOptions.value(shellPluginOption);
 
